{
  "name": "@phanect/utils",
  "version": "1.0.0",
  "description": "@phanect's personal utility library for JavaScript & TypeScript",
  "author": "Jumpei Ogawa <phanective@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=10.x",
    "npm": ">=6.x"
  },
  "type": "module",
  "main": "dist/utils.cjs",
  "exports": {
    "import": "./dist/utils.mjs",
    "require": "./dist/utils.cjs"
  },
  "scripts": {
    "build": "gulp build",
    "lint": "eslint --ext=.js,.cjs,.mjs,.jsx,.ts,.tsx .",
    "release": "npm publish --access public --non-interactive",
    "test": "jest --verbose"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "klona": "^2.0.0",
    "lodash-es": "^4.17.15"
  },
  "devDependencies": {
    "@phanect/eslint-plugin": "latest",
    "@types/gulp": "^4.0.6",
    "@types/gulp-rename": "0.0.33",
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.24",
    "eslint": "latest",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "jest": {
    "clearMocks": true,
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}
